<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Smoke Tests">
    <test thread-count="5" name="DigitalVision-automation-project">
        <classes>
            <class name="tests.RegistrationTest">
                <methods>
                    <include name="registrationValidTest"/>
                </methods>
            </class>
            <class name="tests.LoginTest">
                <methods>
                    <include name="loginPositiveTest"/>
                </methods>
            </class>
            <class name="tests.LogoutTest">
                <methods>
                    <include name="logoutTest"/>
                </methods>
            </class>
            <class name="tests.SearchTest">
                <methods>
                    <include name="searchValidTest"/>
                </methods>
            </class>
            <class name="tests.AddToCartTest">
                <methods>
                    <include name="addToCartTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
